# Backend Environment Variables Example
# Copy this file to .env and fill in your values

# Server Port
PORT=4000

# MongoDB Connection String
MONGO_URI=your_mongodb_connection_string_here

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# Email Configuration
EMAIL=your_email_here
PASS=your_email_app_password_here

# Razorpay Configuration
RZP_KEY_ID=your_razorpay_key_id_here
RZP_KEY_SECRET=your_razorpay_key_secret_here

# CORS Origin (for production deployment)
CORS_ORIGIN=*

# API Variable (if needed)
API_VARI=your_api_variable_here